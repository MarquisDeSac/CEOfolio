---
import Plum from '~/components/backgrounds/Plum.astro'
import Dot from '~/components/backgrounds/Dot.astro'
import Rose from '~/components/backgrounds/Rose.astro'
import Particle from '~/components/backgrounds/Particle.astro'
import RetroGrid from '~/components/backgrounds/RetroGrid.astro'
import BitRain from '~/components/backgrounds/BitRain.astro'
import Sunset from '~/components/backgrounds/Sunset.astro'


import NightDrive from '~/components/backgrounds/NightDrive.astro'
import Starfield from '~/components/backgrounds/Starfield.astro'
import LavaLamp from '~/components/backgrounds/LavaLamp.astro'
import Circuit from '~/components/backgrounds/Circuit.astro'
import Aurora from '~/components/backgrounds/Aurora.astro'
import StaticNoise from '~/components/backgrounds/StaticNoise.astro'


import Equalizer from '~/components/backgrounds/Equalizer.astro'
import Constellation from '~/components/backgrounds/Constellation.astro'
import HoloMesh from '~/components/backgrounds/HoloMesh.astro'


interface Props {
  type: string
}

const { type } = Astro.props
---

{type === 'plum' && <Plum />}
{type === 'dot' && <Dot />}
{type === 'rose' && <Rose />}
{type === 'particle' && <Particle />}
{type === 'retro-grid' && <RetroGrid />}
{type === 'bit-rain' && <BitRain />}
{type === 'sunset' && <Sunset />}
{type === 'night-drive' && <NightDrive />}
{type === 'starfield' && <Starfield />}
{type === 'lava-lamp' && <LavaLamp />}
{type === 'circuit' && <Circuit />}
{type === 'aurora' && <Aurora />}
{type === 'static-noise' && <StaticNoise />}
{type === 'equalizer' && <Equalizer />}
{type === 'constellation' && <Constellation />}
{type === 'holo-mesh' && <HoloMesh />}

